{
  "name": "sensor-manager-api",
  "version": "1.0.0",
  "license": "ISC",
  "description": "",
  "scripts": {
    "build": "tsc --project tsconfig.compile.json && tsc-alias",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn barrels && tsnd --inspect --ignore-watch node_modules --respawn --transpile-only --max-old-space-size=2048 -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "format": "prettier '**/*.{ts,js,json}' --write",
    "prepare": "is-ci || husky install",
    "bootstrap": "yarn start:docker && yarn && yarn prisma:migrate",
    "start:docker": "docker-compose -f docker-compose.dev.yml up -d",
    "clean:docker": "docker-compose -f docker-compose.dev.yml down --volumes --remove-orphans",
    "clean:git": "git branch --merged >/tmp/merged-branches && nano /tmp/merged-branches && xargs git branch -D </tmp/merged-branches && git fetch --prune --all",
    "refresh": "yarn cleanup && yarn start:docker && yarn && yarn prisma:migrate"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@tsed/ajv": "^7.13.0",
    "@tsed/common": "^7.13.0",
    "@tsed/core": "^7.13.0",
    "@tsed/di": "^7.13.0",
    "@tsed/engines": "^7.13.0",
    "@tsed/exceptions": "^7.13.0",
    "@tsed/json-mapper": "^7.13.0",
    "@tsed/logger": "^6.3.3",
    "@tsed/logger-file": "^6.3.3",
    "@tsed/openspec": "^7.13.0",
    "@tsed/passport": "^7.13.0",
    "@tsed/platform-cache": "^7.13.0",
    "@tsed/platform-exceptions": "^7.13.0",
    "@tsed/platform-express": "^7.13.0",
    "@tsed/platform-log-middleware": "^7.13.0",
    "@tsed/platform-middlewares": "^7.13.0",
    "@tsed/platform-params": "^7.13.0",
    "@tsed/platform-response-filter": "^7.13.0",
    "@tsed/platform-views": "^7.13.0",
    "@tsed/prisma": "^7.13.0",
    "@tsed/schema": "^7.13.0",
    "@tsed/socketio": "7.13.0",
    "@tsed/swagger": "^7.13.0",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "method-override": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "prisma": "^4.9.0",
    "socket.io": "^4.5.4",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.2.2",
    "@tsed/cli-plugin-jest": "4.2.2",
    "@tsed/cli-plugin-passport": "4.2.2",
    "@tsed/cli-plugin-prisma": "4.2.2",
    "@tsed/socketio-testing": "7.13.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.6",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "barrelsby": "^2.5.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "socket.io-client": "^4.0.1",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default",
    "architecture": "default"
  }
}
